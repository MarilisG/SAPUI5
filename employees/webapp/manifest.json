{
    "_version": "1.49.0",
    "sap.app": {
        "id": "employees",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "0.0.1"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "resources": "resources.json",
        "sourceTemplate": {
            "id": "@sap/generator-fiori:basic",
            "version": "1.10.6",
            "toolsId": "d6ace9ac-4677-4fce-bee9-bd1a2d239d60"
        },
        "dataSources": {
            "ZEMPLOYEES_SRV": {
                "uri": "/sap/opu/odata/sap/ZEMPLOYEES_SRV/",
                "type": "OData",
                "settings": {
                    "annotations": [],
                    "localUri": "localService/metadata.xml",
                    "odataVersion": "2.0"
                }
            }
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "flexEnabled": false,
        "dependencies": {
            "minUI5Version": "1.118.0",
            "libs": {
                "sap.m": {},
                "sap.ui.core": {},
                "sap.f": {},
                "sap.suite.ui.generic.template": {},
                "sap.ui.comp": {},
                "sap.ui.generic.app": {},
                "sap.ui.table": {},
                "sap.ushell": {}
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "employees.i18n.i18n"
                }
            },
            "odataModel": {
                "preload": true,
                "type": "sap.ui.model.odata.v2.ODataModel",
                "dataSource": "ZEMPLOYEES_SRV",
                "settings": {
                    "defaultOperationMode": "Server",
                    "defaultBindingMode": "OneWay",
                    "defaultCountMode": "Request"
                }
            }
        },
        "resources": {
            "css": [
                {
                    "uri": "css/style.css"
                }
            ]
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "viewType": "XML",
                "async": true,
                "viewPath": "employees.view",
                "controlAggregation": "pages",
                "controlId": "app",
                "clearControlAggregation": false
            },
            "routes": [
                {
                    "name": "RouteMenu",
                    "pattern": ":?query:",
                    "target": [
                        "TargetMenu"
                    ]
                },
                {
                    "name": "CreateEmployee",
                    "pattern": "CreateEmployee",
                    "target": [
                        "TargetCreateEmployee"
                    ]
                },
                {
                    "name": "ShowEmployee",
                    "pattern": "ShowEmployee",
                    "target": [
                        "TargetShowEmployee"
                    ]
                }                 
            ],
            "targets": {
                "TargetMenu": {
                    "viewType": "XML",
                    "transition": "slide",
                    "clearControlAggregation": true,
                    "viewId": "Menu",
                    "viewName": "Menu"
                },
                "TargetCreateEmployee": {
                    "viewType": "XML",
                    "transition": "slide",
                    "clearControlAggregation": true,
                    "viewId": "CreateEmployee",
                    "viewName": "CreateEmployee"
                },
                "TargetShowEmployee": {
                    "viewType": "XML",
                    "transition": "slide",
                    "clearControlAggregation": true,
                    "viewId": "ShowEmployee",
                    "viewName": "ShowEmployee"
                }                 
            }
        },
        "rootView": {
            "viewName": "employees.view.App",
            "type": "XML",
            "async": true,
            "id": "App"
        }
    }
}